<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.gruporon2005.aquarius.bean">
  <class name="Store" table="store">
    <id column="store_id" name="id">
      <generator class="native"/>
    </id>
    <property column="nemo" name="nemo" type="string"/>
    <property column="requester" name="requester" type="string"/>
    <property column="endpoint" name="apiUrl" type="string"/>
    <property column="magento_store_id" name="magentoStoreId" type="integer"/>
  </class>
</hibernate-mapping>
